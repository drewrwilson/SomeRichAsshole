{
	"manifest_version": 2,
	"name": "Some Rich Asshole Enhanced",
	"version": "3.0.4",
	"description": "Replaces He-Who-Must-Not-Be-Named's name, and those of some related individuals, with more colorful descriptors.",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
      "css": ["actionstations.css"],
			"js": ["jquery-3.1.1.min.js", "jquery-ui/jquery-ui.min.js", "jquery.tcycle.js", "content_script.js", "actionstations.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
          "128": "icon128.png" }
}
